{
  "tasks": {
    "start": "deno run --unstable --allow-env --allow-net --allow-read mod.ts",
    "dev": "deno run --unstable --watch --allow-env --allow-net --allow-read mod.ts",
    "check": "deno check --unstable mod.ts",
    "lint": "deno lint",
    "test": "deno test",
    "cache": "deno cache mod.ts",
    "fmt": "deno run -A npm:@biomejs/biome format --check .",
    "compile": "deno compile --unstable -A mod.ts"
  },
  "imports": {
    "@commands/": "./src/commands/",
    "@components/": "./src/components/",
    "@core/": "./src/core/",
    "@events/": "./src/events/",
    "@framework/": "./src/framework/",
    "@utils/": "./src/utils/",
    "canvas/": "https://deno.land/x/canvas@v1.4.1/",
    "gtr/": "https://deno.land/x/gtr@v0.0.1/",
    "harmony/": "https://deno.land/x/harmony@v2.9.0/",
    "ky": "https://esm.sh/ky@1.2.4",
    "std/": "https://deno.land/std@0.223.0/"
  },
  "lock": false
}
